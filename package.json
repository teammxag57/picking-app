    "@zxing/browser": "^0.1.5",
    "html5-qrcode": "^2.3.8",
    "isbot": "^5.1.31",
    "prisma": "^6.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.13.0",
